{
  "expo": {
    "name": "zeko-fix",
    "slug": "zeko-fix",
    "version": "1.0.0",
    "owner": "tinnanndo",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "android": {
      "package": "com.tinnanndo.zekofix",
      "permissions": [
        "ACTIVITY_RECOGNITION",
        "FOREGROUND_SERVICE",
        "WAKE_LOCK"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "foregroundService": true
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "0ec072cd-23a9-4f55-bdc5-8723b79b9bc4"
      }
    }
  }
}